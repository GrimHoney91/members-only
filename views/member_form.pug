extends layout 

block content
    form(action="" method='POST')
        h1=title 
        div.form-group
            label(for="passcode") Member passcode:
            input#passcode.form-control(type="text" name="passcode" placeholder='Member passcode' required='true' minLength='5' maxLength='30')
        input(type="hidden" name='userid' value=currentUser._id) 
        button.submit-btn(type="submit") Enter